<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assignmentrecord.labgroup.finalyearprojectdemo">

    <application
        android:name=".Application.GlobalAppSettings"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MaterialTheme">
        <activity android:name=".Activity.LabsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.GroupsActivity" />
        <activity android:name=".Activity.StudentsActivity" />
        <activity android:name=".Activity.AssignmentActivity"></activity>
    </application>

</manifest>